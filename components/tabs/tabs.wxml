<view class="tabs">
      <view wx:for="{{ tabs }}" wx:key="index" class="{{ index === mine ? 'tab-active' : 'tab'}}" bindtap="{{index === mine ? 'toMine' : 'onTab'}}" data-index="{{ index }}">{{ item.label }}</view>
      <view class="line" style="{{ 'width: ' + linePositionWidth + 'px; left: ' + linePositionX + 'px' }}"></view>
</view>